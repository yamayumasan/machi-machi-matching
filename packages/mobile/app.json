{
  "expo": {
    "name": "マチマチマッチング",
    "slug": "machi-machi-matching",
    "version": "0.1.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "scheme": "machi-machi",
    "owner": "ymab",
    "splash": {
      "image": "./assets/splash-icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": false,
      "bundleIdentifier": "com.machimachi.matching",
      "buildNumber": "11",
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "近くの募集やユーザーを表示するために位置情報を使用します",
        "NSLocationAlwaysUsageDescription": "近くの募集やユーザーを表示するために位置情報を使用します",
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ef4444"
      },
      "edgeToEdgeEnabled": true,
      "package": "com.machimachi.matching",
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION"
      ]
    },
    "plugins": [
      "expo-router",
      "expo-secure-store",
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "近くの募集やユーザーを表示するために位置情報を使用します"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "ea2cefcf-9ae6-4f7d-84f5-1c299afeb996"
      }
    }
  }
}
